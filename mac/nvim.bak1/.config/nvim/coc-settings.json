{
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.suggestionActions.enabled": true,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.suggestionActions.enabled": true,
  "javascript.showUnused": true,
  "tsserver.useLocalTsdk": true,
  "suggest.noselect": true,
  "session.saveOnVimLeave": true,
  "go.enable": true,
  "go.goplsPath": "/Users/yassinebridi/go/bin/gopls",
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": [
        "server",
        "-m",
        "stream"
      ],
      "filetypes": [
        "typescript",
        "typescriptreact",
        "graphql"
      ]
    }
  },
  "coc.preferences.extensionUpdateCheck": "never",
  "prismaLanguageServer.prismaFmtBinPath": "",
  "coc.preferences.formatOnType": true,
  "prisma.prismaFmtBinPath": "",
  "coc.preferences.formatOnSaveFiletypes": [
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql",
    "python",
    "mdx",
    "md",
    "ejs",
    "toml",
    "json",
    "c",
    "rust",
    "css",
    "prisma",
    "go",
    "svelte",
    "yaml"
  ],
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.width": 30,
  "explorer.keyMappings.global": {
    "ga": "collapse:all",
    ".": "toggleHidden"
  },
  "yaml.format.enable": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/GoogleContainerTools/skaffold/main/docs-v2/content/en/schemas/v4beta5.json": [ "skaffold.yaml" ]
  },
  "yaml.format.singleQuote": true
}
