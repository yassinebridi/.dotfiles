#!/bin/sh

if [[ $1 = d1 ]]; then
        bspc node -d ^1
elif [[ $1 = d2 ]]; then
        bspc node -d ^2
elif [[ $1 = d3 ]]; then
        bspc node -d ^3
elif [[ $1 = d4 ]]; then
        bspc node -d ^4
elif [[ $1 = d5 ]]; then
        bspc node -d ^5
elif [[ $1 = d6 ]]; then
        bspc node -d ^6
elif [[ $1 = d7 ]]; then
        bspc node -d ^7
elif [[ $1 = d8 ]]; then
        bspc node -d ^8
elif [[ $1 = d9 ]]; then
        bspc node -d ^9
else
        echo
fi
